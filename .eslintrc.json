{
  "parser": "@typescript-eslint/parser",
  "plugins": ["filenames", "@typescript-eslint"],
  "rules": {
    "filenames/match-regex": [2, "^[a-z0-9\\-\\.]+$", true]
  },
  "ignorePatterns": ["*.d.ts", "*.js"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "parserOptions": {
        "project": "./tsconfig.json"
      }
    }
  ]
}
